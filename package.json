{
  "name": "jaam",
  "version": "1.0.3",
  "description": "utility functions shared between libraries",
  "npmName": "jaam",
  "scripts": {
    "test": "ava",
    "test:coverage": "nyc ava",
    "check": "npm-check -s -u",
    "lint": "eslint ./js",
    "lint:fix": "eslint ./js --fix",
    "prepush": "npm-run-all -s lint test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codebend3r/jaam.git"
  },
  "keywords": [
    "utility",
    "helper"
  ],
  "files": [
    "js"
  ],
  "author": "Chester Rivas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codebend3r/jaam/issues"
  },
  "homepage": "https://github.com/codebend3r/jaam#readme",
  "devDependencies": {
    "ava": "0.25.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "eslint": "4.19.0",
    "eslint-config-idiomatic": "4.0.0",
    "eslint-friendly-formatter": "3.0.0",
    "husky": "0.14.3",
    "jsdom": "11.6.2",
    "lodash": "4.17.5",
    "npm-check": "5.5.2",
    "npm-run-all": "4.1.2",
    "nyc": "11.6.0",
    "sinon": "4.4.6",
    "wolfy87-eventemitter": "5.2.4"
  },
  "ava": {
    "inherit": true,
    "failFast": true,
    "concurrency": 5,
    "compileEnhancements": true,
    "files": [
      "./test/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "verbose": true
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  }
}
